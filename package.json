{
  "name": "react-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --fix src/",
    "prettier": "prettier --write src/**/*.tsx",
    "test": "vitest --coverage",
    "build:client": "vite build --outDir ./dist/client",
    "build:server": "vite build --outDir ./dist/server --ssr src/ServerApp.tsx",
    "build": "npm run build:client && npm run build:server",
    "start:server": "ts-node --experimental-specifier-resolution=node ./server.tsx",
    "cypress:open": "cypress open --e2e --browser chrome",
    "cypress:coverage": "npx nyc report --reporter=text-summary",
    "cypress:e2e": "start-server-and-test start:server http-get://localhost:3001 cypress:open",
    "cypress:e2e2": "start-server-and-test start:server http://localhost:3001/ cypress:open && npx nyc report --reporter=text-summary"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "express": "^4.18.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "msw": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sort-by": "^0.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "cypress": "^12.10.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.61.0",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-plugin-linter": "^2.0.2",
    "vitest": "^0.29.3"
  },
  "eslintIgnore": [
    "/dist"
  ]
}
